doctype html
html(lang='da')
  head
    meta(charset='UTF-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    link(rel='icon', type='image/webp', sizes='32x32', href='images/')
    link(rel='preconnect', href='https://fonts.googleapis.com')
    link(rel='preconnect', href='https://fonts.gstatic.com', crossorigin='')
    link(href='https://fonts.googleapis.com/css2?family=Bangers&family=Comic+Neue:ital,wght@1,700&family=Lobster&display=swap', rel='stylesheet')
    title Skats positivliste
body(class='')
  main(class='')
    p #{new Date(file.mtime).toLocaleDateString('da-DK')}
    table
      tbody
        tr
          each column in columns
            th= column
          each value in values
            tr
              each column in columns
                td= value[column]